{
	"name": "<<name>>",
	"version": "<<version>>",
	"manifest_version": 2,
	"description": "<<description>>",
	"icons": {
		"16": "<<icon_file>>"
	},
	"content_scripts": [{
		"js": ["<<script>>"],
		"matches": <<json:urls>>,
		"all_frames": true,
		"run_at": "document_start"
	}],
	"homepage_url": "<<homepage>>",
	"permissions": [
		"storage",
		"http://boards.4chan.org/",
		"https://boards.4chan.org/",
		"http://archive.foolz.us/",
		"https://archive.foolz.us/",
		"http://boards.38chan.net/",
		"https://boards.38chan.net/",
		"http://i.4cdn.org/",
		"https://i.4cdn.org/",
		"http://a.4cdn.org/",
		"https://a.4cdn.org/",
		"http://*.foolz.us/",
		"https://*.foolz.us/"
	]
}